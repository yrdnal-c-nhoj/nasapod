{
  "version": 2,
  "builds": [
    {
      "src": "api/keepalive.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/api/keepalive", "dest": "/api/keepalive" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ],
  "crons": [
    {
      "path": "/api/keepalive",
      "schedule": "*/10 * * * *"
    }
  ]
}
